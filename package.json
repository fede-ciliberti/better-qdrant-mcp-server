    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "fastembed": "^1.1.0",
    "langchain": "^0.3.19",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
